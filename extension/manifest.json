{
    "manifest_version": 3,
    "name": "Job Scraper",
    "version": "1.0",
    "description": "Scrape and email jobs via your local Job Scraper API",
    "permissions": [
      "storage"
    ],
    "host_permissions": [
      "http://127.0.0.1:5000/*"
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
  